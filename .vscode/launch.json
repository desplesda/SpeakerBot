	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"runtimeVersion": "16",
			"request": "launch",
			"name": "Launch Program",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/speaker.js",
			"outFiles": [
				"${workspaceFolder}/**/*.js"
			]
		},
		{
			"type": "node",
			"runtimeVersion": "16",
			"request": "launch",
			"name": "Deploy Commands",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/deploy-commands.js",
			"outFiles": [
				"${workspaceFolder}/**/*.js"
			]
		}

	]
}